@PACKAGE_INIT@

# Set include directories
set_and_check(SDL_Graph_INCLUDE_DIRS "@PACKAGE_INCLUDE_INSTALL_DIR@")

# Set library locations
set_and_check(SDL_Graph_LIBRARIES "@PACKAGE_LIB_INSTALL_DIR@/libSDL_Graph.a")  # Adjust for your system (e.g., .lib on Windows)

# Dependencies
find_dependency(SDL2 REQUIRED)
find_dependency(SDL2_ttf REQUIRED)

# Include targets file
include("${CMAKE_CURRENT_LIST_DIR}/SDL_GraphTargets.cmake")

# Mark as found
set(SDL_Graph_FOUND TRUE)